{
    "plugins": [
        "react"
    ],
    "extends": ["plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true,
            "jsx": true
        }
    },
    "env": {
        "node": false,
        "browser": true,
        "commonjs": true,
        "es6": true
    }
}
